{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "db/db-connection/(.*)": "<rootDir>/../libs/db-connection/src/$1",
    "db/db-connection": "<rootDir>/../libs/db-connection/src"
  }
}